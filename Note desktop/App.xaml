<Application x:Class="Note_desktop.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Note_desktop"
             StartupUri="View/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="ResourceDictionaries/Button.xaml"/>
                <ResourceDictionary Source="ResourceDictionaries/Color.xaml"/>
                <ResourceDictionary Source="ResourceDictionaries/ScrollViewer.xaml"/>
                <ResourceDictionary Source="ResourceDictionaries/TextBlock.xaml"/>
                <ResourceDictionary Source="ResourceDictionaries/TextBox.xaml"/>
                <ResourceDictionary Source="ResourceDictionaries/Window.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
